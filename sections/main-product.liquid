{% schema %}
{
  "name": "t:sections.main-product.name",
  "tag": "section",
  "class": "section",
  "blocks": [
    {
      "type": "accordion",
      "name": "Accordions",
      "settings":[
        {
          "type": "text",
          "id": "title",
          "label": "Description Tab",
          "default": "Product Description"
        },
        {
          "type": "text",
          "id": "title2",
          "label": "Tab 2 Title",
          "default": "Shipping & Returns"
        },
        {
          "type": "richtext",
          "id": "text",
          "label": "Tab 2 Text"
        }
      ]
    },
    {
      "type": "@app"
    },
    {
      "type": "text",
      "name": "t:sections.main-product.blocks.text.name",
      "settings": [
        {
          "type": "inline_richtext",
          "id": "text",
          "default": "Text block",
          "label": "t:sections.main-product.blocks.text.settings.text.label"
        },
        {
          "type": "select",
          "id": "text_style",
          "options": [
